<title>getJSON</title>
<head>
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
<script>
$.getJSON( "aa.json", function( data ) {
	for(i=0;i<data.length;i++){
	console.log(data[i].Name);
	$("body").append(data[i].Name+"</br>");	
	}
	
});

</script>
</head>
